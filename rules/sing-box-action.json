{
  "dns": {
    "fakeip": {
      "enabled": true,
      "inet4_range": "198.18.0.0/15",
      "inet6_range": "fc00::/18"
    },
    "servers": [
      {
        "tag": "google",
        "address": "https://8.8.8.8/dns-query",
        "detour": "proxy"
      },
      {
        "tag": "tx",
        "address": "https://120.53.53.53/dns-query",
        "detour": "direct"
      },
      {
        "tag": "fakeip",
        "address": "fakeip"
      },
      {
        "tag": "nxdomain",
        "address": "rcode://success"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "tx"
      },
      {
        "rule_set": [
          "AWAvenue-Ads",
          "广告拦截"
        ],
        "action": "route",
        "server": "nxdomain",
        "disable_cache": true
      },
      {
        "clash_mode": "Direct",
        "action": "route",
        "server": "tx"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "server": "fakeip"
      },
      {
        "rule_set": "强制代理-域名",
        "action": "route",
        "server": "google"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": [
          "geosite-geolocation-!cn",
          "emby"
        ],
        "action": "route-options",
        "server": "fakeip",
        "rewrite_ttl": 1
      },
      {
        "rule_set": [
          "geosite-geolocation-cn",
          "wechat"
        ],
        "action": "route",
        "server": "tx"
      },
      {
        "type": "logical",
        "mode": "and",
        "rules": [
          {
            "rule_set": "geosite-geolocation-!cn",
            "invert": true
          },
          {
            "rule_set": "geoip-cn"
          }
        ],
        "action": "route-options",
        "server": "google",
        "client_subnet": "114.114.114.114"
      }
    ],
    "strategy": "prefer_ipv4",
    "independent_cache": true
  },
  "route": {
    "rules": [
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 853
          },
          {
            "network": "udp",
            "port": 443
          },
          {
            "protocol": "stun"
          }
        ],
        "action": "reject"
      },
      {
        "rule_set": [
          "AWAvenue-Ads",
          "广告拦截"
        ],
        "action": "reject"
      },
      {
        "ip_is_private": true,
        "outbound": "direct"
      },
      {
        "clash_mode": "Direct",
        "action": "route",
        "outbound": "direct"
      },
      {
        "clash_mode": "Global",
        "action": "route",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": "emby",
        "action": "route",
        "outbound": "emby"
      },
      {
        "rule_set": "youtube",
        "action": "route",
        "outbound": "youtube"
      },
      {
        "rule_set": "openai",
        "action": "route",
        "outbound": "openai"
      },
      {
        "rule_set": [
          "强制代理-域名",
          "geoip-google",
          "geosite-google"
        ],
        "action": "route",
        "outbound": "google"
      },
      {
        "rule_set": [
          "geoip-telegram",
          "geosite-telegram"
        ],
        "action": "route",
        "outbound": "telegram"
      },
      {
        "domain_suffix": "eslink.cc",
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": [
          "geoip-cn",
          "wechat",
          "geosite-geolocation-cn"
        ],
        "action": "route",
        "outbound": "direct"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
        "action": "route",
        "outbound": "proxy"
      }
    ],
    "rule_set": [
      {
        "tag": "广告拦截",
        "url": "https://github.com/Toperlock/sing-box-geosite/raw/main/rule/adrules_domainset.srs",
        "type": "remote",
        "format": "binary",
        "download_detour": "proxy",
        "update_interval": "24h"
      },
      {
        "tag": "AWAvenue-Ads",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/TG-Twilight/AWAvenue-Ads-Rule/main/Filters/AWAvenue-Ads-Rule-Singbox.srs",
        "download_detour": "proxy",
        "update_interval": "24h"
      },
      {
        "tag": "emby",
        "type": "local",
        "format": "source",
        "path": "/storage/emulated/0/Android/data/io.nekohasekai.sfa/files/RuleProviders/emby.json"
      },
      {
        "tag": "wechat",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://raw.githubusercontent.com/Toperlock/sing-box-geosite/main/rule/WeChat.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geoip-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/SagerNet/sing-geoip/raw/rule-set/geoip-cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-geolocation-cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/SagerNet/sing-geosite/raw/rule-set/geosite-geolocation-cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geoip-google",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geoip/google.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geosite/google.srs",
        "download_detour": "direct"
      },
      {
        "tag": "强制代理-域名",
        "path": "/storage/emulated/0/Android/data/io.nekohasekai.sfa/files/RuleProviders/强制代理-域名.json",
        "type": "local",
        "format": "source"
      },
      {
        "tag": "geosite-geolocation-!cn",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/SagerNet/sing-geosite/raw/rule-set/geosite-geolocation-!cn.srs",
        "download_detour": "direct"
      },
      {
        "tag": "openai",
        "type": "remote",
        "format": "binary",
        "url": "https://ghfast.top/https://github.com/Toperlock/sing-box-geosite/raw/refs/heads/main/rule/OpenAI.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geoip-telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geoip/telegram.srs",
        "download_detour": "direct"
      },
      {
        "tag": "geosite-telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geosite/telegram.srs",
        "download_detour": "direct"
      },
      {
        "tag": "youtube",
        "type": "remote",
        "format": "binary",
        "url": "https://fastly.jsdelivr.net/gh/MetaCubeX/meta-rules-dat@sing/geo-lite/geosite/youtube.srs",
        "download_detour": "direct"
      }
    ],
    "final": "proxy",
    "auto_detect_interface": true
  },
  "outbounds": [
    {
      "tag": "proxy",
      "type": "selector",
      "outbounds": [
        "google",
        "youtube",
        "telegram",
        "openai",
        "all",
        "direct"
      ],
      "default": "all",
      "interrupt_exist_connections": true
    },
    {
      "tag": "google",
      "type": "selector",
      "outbounds": [
        "us",
        "all"
      ],
      "default": "us"
    },
    {
      "tag": "youtube",
      "type": "selector",
      "outbounds": [
        "sg",
        "us",
        "all"
      ],
      "default": "us"
    },
    {
      "tag": "telegram",
      "type": "selector",
      "outbounds": [
        "sg",
        "us",
        "all"
      ],
      "default": "sg"
    },
    {
      "tag": "emby",
      "type": "selector",
      "outbounds": [
        "direct",
        "sg",
        "us",
        "all"
      ],
      "default": "all"
    },
    {
      "tag": "openai",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "all",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "us",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "sg",
      "type": "selector",
      "outbounds": []
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [
        "direct",
        "proxy",
        "youtube",
        "google",
        "telegram",
        "emby",
        "openai",
        "all",
        "us",
        "sg"
      ],
      "default": "direct"
    },
    {
      "tag": "direct",
      "type": "direct"
    }
  ],
  "inbounds": [
    {
      "type": "tun",
      "address": [
        "172.19.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "stack": "system",
      "auto_route": true,
      "strict_route": true,
      "sniff": true,
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7890
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7890,
      "sniff": true
    }
  ],
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://github.com/MetaCubeX/metacubexd/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "proxy"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": true
    }
  },
  "log": {
    "disabled": false,
    "level": "info",
    "output": "box.log",
    "timestamp": true
  }
}
